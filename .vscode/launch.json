{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: 调试当前文件（可进入库源码）",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": false,
      "debugStdLib": true,
      "subProcess": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "python.languageServer": "Pylance"
    }
  ]
}
